<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:id="@+id/glview_container"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_margin="40.0dip"
        android:alpha="0.0" />

    <RelativeLayout
        android:id="@+id/main_rel"
        android:visibility="visible"
        android:layout_width="fill_parent"
        android:layout_height="1080.0px">

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="fill_parent"
            android:layout_height="540.0px"
            android:translationX="0.0px"
            android:translationY="0.0px">

            <RelativeLayout
                android:id="@+id/rl3"
                android:layout_width="360.0px"
                android:layout_height="fill_parent"
                android:layout_marginLeft="0.0px"
                android:translationY="0.0px">

                <mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout
                    android:id="@+id/fl1"
                    android:background="@color/colorFrameBg"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <mobo.andro.apps.ohmagiccamera.CollageMaker.GLFilterImageView
                        android:layout_gravity="center"
                        android:id="@+id/img1"
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/add_image" />
                </mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout>
            </RelativeLayout>

            <View
                android:id="@+id/div2"
                android:background="@color/red"
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_marginLeft="360.0px"
                android:translationY="0.0px" />

            <RelativeLayout
                android:id="@+id/rl4"
                android:layout_width="360.0px"
                android:layout_height="fill_parent"
                android:translationX="360.0px"
                android:translationY="0.0px">

                <mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout
                    android:id="@+id/fl2"
                    android:background="@color/colorFrameBg"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <mobo.andro.apps.ohmagiccamera.CollageMaker.GLFilterImageView
                        android:layout_gravity="center"
                        android:id="@+id/img2"
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/add_image" />
                </mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout>
            </RelativeLayout>

            <View
                android:id="@+id/div3"
                android:background="@color/red"
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_marginLeft="720.0px"
                android:translationY="0.0px" />

            <RelativeLayout
                android:id="@+id/rl5"
                android:layout_width="360.0px"
                android:layout_height="fill_parent"
                android:translationX="720.0px"
                android:translationY="0.0px">

                <mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout
                    android:id="@+id/fl3"
                    android:background="@color/colorFrameBg"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <mobo.andro.apps.ohmagiccamera.CollageMaker.GLFilterImageView
                        android:layout_gravity="center"
                        android:id="@+id/img3"
                        android:visibility="visible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/add_image" />
                </mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout>
            </RelativeLayout>
        </RelativeLayout>

        <View
            android:id="@+id/div1"
            android:background="@color/red"
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:translationX="0.0px"
            android:translationY="540.0px" />

        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="fill_parent"
            android:layout_height="540.0px"
            android:translationX="0.0px"
            android:translationY="540.0px">

            <mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout
                android:id="@+id/fl4"
                android:background="@color/colorFrameBg"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <mobo.andro.apps.ohmagiccamera.CollageMaker.GLFilterImageView
                    android:layout_gravity="center"
                    android:id="@+id/img4"
                    android:visibility="visible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/add_image" />
            </mobo.andro.apps.ohmagiccamera.CollageMaker.RoundCornerFrameLayout>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>